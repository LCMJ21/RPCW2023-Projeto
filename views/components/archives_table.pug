block table
    table.table
        thead
            tr
                th.col Id
                th.col Accordion
                th.col Autor
                th.col Data
                th.col Topic
                th.col Court
                th.col
        tbody
            each elem in table_data.results
                tr
                    th(scope="row") #{elem.id}
                    td #{elem.Acord√£o}
                    td #{elem.Autor}
                    td #{elem.Data}
                    td #{elem.Tema}
                    td #{elem.Tribunal}
                    td.text-end
                        if elem.id in user.favorites
                            input.btn-check(type="checkbox" id="favorite-" + elem.id checked)
                            label.btn.btn-outline-warning(for="favorite-" + elem.id)
                                i.bi.bi-star
                        else
                            input.btn-check(type="checkbox" id="favorite-" + elem.id)
                            label.btn.btn-outline-warning(for="favorite-" + elem.id)
                                i.bi.bi-star

    block pagination
        include pagination
