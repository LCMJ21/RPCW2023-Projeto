block pagination
    - var query = "&" + oldurl 
    .d-flex.flex-row.align-items-center
        p.mb-0 #{data_name} nÂº #{table_data.currentElems-table_data.results.length} atÃ© #{table_data.currentElems} 

        nav.ms-auto
            ul.pagination.mb-0
                if table_data.currentPage != 1
                    li.page-item
                        if table_data.currentPage > 10
                            a.page-link(href="?page=" + (table_data.currentPage - 10) + query) &lt;&lt;
                        else
                            a.page-link(href="?page=1" + query) &lt;&lt;
                    li.page-item
                        a.page-link(href="?page=" + (table_data.currentPage - 1) + query) &lt;

                if table_data.previousPages
                    each page in table_data.previousPages
                        li.page-item
                            a.page-link(href="?page=" + page + query) #{page}

                li.page-item.active
                    a.page-link.disable() #{table_data.currentPage}
                
                if table_data.nextPages
                    each page in table_data.nextPages
                        li.page-item
                            a.page-link(href="?page=" + page + query) #{page}
                
                if table_data.currentPage != table_data.totalPages
                    li.page-item
                        a.page-link(href="?page=" + (table_data.currentPage + 1) + query) &gt;
                    li.page-item
                        a.page-link(href="?page=" + table_data.totalPages + query) &gt;&gt;
                        